{
  "name": "@teacss/postcss-whitespace",
  "version": "0.1.0-alpha.1",
  "license": "MIT",
  "sideEffects": false,
  "description": "postcss-whitespace",
  "author": "Billgo <hi@billgo.me>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "style": "./index.css",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "bun run build",
    "build": "bunx rimraf dist && bun run build:esm && bun run build:bun",
    "build:bun": "bun run bun.build.mjs",
    "build:esm": "bunx tsup"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@devjskit/biome-config": "^0.2.1",
    "@devjskit/rimraf": "^1.0.0",
    "@types/bun": "^1.2.19",
    "@types/web": "^0.0.253",
    "postcss": "^8.5.6",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
